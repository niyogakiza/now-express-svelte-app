{
  "version": 2,
  "name": "express-svelte-app",
  "regions": ["dub1"],
  "builds": [
    {
      "src": "api/index.js",
      "use": "@now/node"
    },
    {
      "src": "app/package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "public"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    }
  ],
  "github": {
    "silent": true
  }
}
